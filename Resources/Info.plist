<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>$(PRODUCT_NAME)</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0</string>
	<key>CFBundleVersion</key>
	<string>1</string>
	<key>LSRequiresIPhoneEnvironment</key>
	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
	</array>
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>armv7</string>
	</array>
	<key>NSUserTrackingUsageDescription</key>
	<string>FitConnect uses tracking to provide personalized fitness recommendations and improve your workout experience. Your data is kept anonymous and secure.</string>
	<key>NSHealthShareUsageDescription</key>
	<string>FitConnect needs access to your health data like step count, active energy burned, and water intake to track your progress and provide personalized insights.</string>
	<key>NSHealthUpdateUsageDescription</key>
	<string>FitConnect needs to update your health data to keep your records consistent.</string>
	<key>NSCameraUsageDescription</key>
	<string>FitConnect needs camera access to scan and analyze your meals for nutritional information and record workout videos to share with your fitness team.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>FitConnect needs microphone access to record audio with your workout videos for better communication with your fitness team.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>FitConnect needs access to your photo library to select meal photos for nutritional analysis and to save workout videos.</string>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsLocalNetworking</key>
		<true/>
		<key>NSExceptionDomains</key>
		<dict>
			<key>localhost</key>
			<dict>
				<key>NSIncludesSubdomains</key>
				<true/>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<true/>
			</dict>
			<key>127.0.0.1</key>
			<dict>
				<key>NSIncludesSubdomains</key>
				<true/>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<true/>
			</dict>
		</dict>
	</dict>
	<key>LSApplicationCategoryType</key>
	<string></string>
	<key>UIApplicationSceneManifest</key>
	<dict>
		<key>UIApplicationSupportsMultipleScenes</key>
		<false/>
		<key>UISceneConfigurations</key>
		<dict/>
	</dict>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleTypeRole</key>
			<string>Editor</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>com.googleusercontent.apps.932584197048-h50a1adm05o6v6tjk0b4p8898cbeebus</string>
			</array>
		</dict>
	</array>
</dict>
</plist>
